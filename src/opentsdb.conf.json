  "meta": "FILELIST"
 },
 {
  "key": "tsd.storage.hbase.tree_table",
  "cl-option": "--treetable",
  "defaultValue": "tsdb-tree",
  "description": "The name of the tree data table",
  "help": "extended",
  "meta": "TABLE"
 },
 {
  "key": "tsd.network.keep_alive",
  "cl-option": "--keep-alive",
