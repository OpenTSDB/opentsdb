  "help": "default"
 },
 {
  "key": "tsd.storage.enable_compaction",
  "cl-option": "--enable-compaction",
  "defaultValue": "true",
  "description": "Enables periodic storage compaction",
  "help": "extended",
  "meta": "BOOL"
 },
 {
  "key": "tsd.core.meta.enable_realtime_uid",
  "cl-option": "--realtime-uid",
  "defaultValue": "false",
