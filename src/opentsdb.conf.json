  "help": "extended"
 },
 {
  "key": "tsd.core.tree.enable_processing",
  "cl-option": "--enable-tree",
  "defaultValue": "false",
  "description": "Enables TSDB Tree processing",
  "help": "extended"
 },
 {
  "key": "tsd.stats.canonical",
  "cl-option": "--stats-canonical",
  "defaultValue": "false",
