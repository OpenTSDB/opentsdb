  "help": "extended",
  "meta": "POSINT"
 },
 {
  "key": "tsd.http.show_stack_trace",
  "cl-option": "--show-stack",
  "defaultValue": "false",
  "description": "If true, stack traces will be sent to the http client caller when an error occurs",
  "help": "extended"
 },
 {
  "key": "tsd.core.auto_create_metrics",
  "cl-option": "--auto-metric",
  "defaultValue": "true",
